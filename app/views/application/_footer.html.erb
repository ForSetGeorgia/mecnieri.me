
<footer class='app-footer-wrapper'>
	<div class='app-footer container'>
		<div class="footer-left addthis_toolbox addthis_default_style"  addthis:url="<%= root_url %>">
			<a class="addthis_button_facebook_like" fb:like:layout="button_count"></a>
		</div>

		<div class="fb-follow show-follow" data-href="https://facebook.com/JumpStartGeorgia/" data-layout="standard" data-size="small" data-show-faces="false"></div>

		<div class="fb-follow hide-follow" data-href="https://facebook.com/JumpStartGeorgia/" data-layout="button_count" data-size="small" data-show-faces="false"></div>
		<div class='default'>
			<p class='date'>&copy; <%= Time.now.year %></p>
			<%= link_to image_tag('jumpstart_logo.svg'),
			              'http://jumpstart.ge',
			              target: '_blank',
			              title: t('shared.common.jumpstart.slogan') %>
		</div>
	</div>


</footer>

<div id="fb-root"></div>

